<?xml version="1.0" encoding="utf-8"?>
<!-- This file along with its dll needs to be installed to ...InstallDir...\RuntimeData\FormulaAddIn to run. -->
<FormulaAddIn>
  <Function>
    <Name>CleanNulls</Name>
    <NumParams variable="false">1</NumParams>
    <Category>Conversion</Category>
    <InsertText>CleanNulls(x)</InsertText>
    <Description>Converts null values to the corresponding data type's zero-value (0 for numbers and '' for strings)</Description>
    <Dll>
      <Name>cleannulls.dll</Name>
      <EntryPoint>CleanNulls</EntryPoint>
    </Dll>
  </Function>
</FormulaAddIn>
